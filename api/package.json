{
  "name": "survey_assure_api",
  "version": "1.0.1",
  "description": "Survey aggregation & audit software with Blockchain immutability built in.",
  "engineStrict": true,
  "engines": {
    "node": ">= 10.13 <11"
  },
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "devDependencies": {
    "@types/jest": "~23.3.9",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/node": "^10.17.21",
    "@types/superagent": "^4.1.7",
    "jest": "~29.3.1",
    "prettier": "1.15.2",
    "readline": "^1.3.0",
    "rimraf": "~2.6.2",
    "supertest": "^3.3.0",
    "ts-jest": "~23.10.5",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "1.16.0",
    "tslint-microsoft-contrib": "~5.2.1",
    "tsutils": "~3.5.0"
  },
  "scripts": {
    "bc:deploy": "node contracts/deploy.js",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "clean": "rimraf coverage build tmp",
    "db:build": "node -r dotenv/config db/build.js",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "db:reset": "knex migrate:rollback && knex migrate:latest && knex seed:run",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "postinstall": "npm run build && npm run db:migrate",
    "start": "node -r dotenv/config build/src/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "Jon Belelieu",
  "license": "MIT",
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "adm-zip": "^0.4.14",
    "bcrypt": "3.0.8",
    "buffer-reverse": "^1.0.1",
    "crypto": "^1.0.1",
    "crypto-js": "^3.3.0",
    "dotenv": "^6.1.0",
    "ethers": "^4.0.47",
    "he": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.5",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-jwt": "^3.5.1",
    "koa-locale": "^1.3.0",
    "koa-logger": "^3.2.0",
    "koa-mount": "^4.0.0",
    "koa-qs": "^2.0.0",
    "koa-ratelimit": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "merkletreejs": "^0.1.11",
    "moment": "^2.24.0",
    "node-input-validator": "^4.2.0",
    "pg": "7.12.1",
    "redis": "^2.8.0",
    "superagent": "^5.2.2",
    "tslib": "~1.9.3",
    "treeify": "^1.1.0",
    "typescript": "~3.2.1"
  }
}